<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/holo_red_light"
    tools:context=".endActivity">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="294dp"
        android:layout_height="349dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="81dp"
        android:layout_marginEnd="59dp"
        android:layout_marginBottom="36dp"
        app:layout_constraintBottom_toTopOf="@+id/eatvName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bricksben"
        android:contentDescription="TODO" />

    <Button
        android:id="@+id/eabutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="161dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="162dp"
        android:layout_marginBottom="112dp"
        android:background="@color/colorAccent"
        android:fontFamily="@font/delius_swash_caps"
        android:text="Home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eatvName" />

    <TextView
        android:id="@+id/eatvName"
        android:layout_width="369dp"
        android:layout_height="50dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="63dp"
        android:layout_marginBottom="35dp"
        android:fontFamily="@font/amaranth"
        android:text="Greetings"
        android:textColor="@color/colorAccent"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/eabutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

</androidx.constraintlayout.widget.ConstraintLayout>